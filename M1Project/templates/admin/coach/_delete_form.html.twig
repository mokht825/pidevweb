<form method="post" action="{{ path('coach_delete', {'Idcoach': coach.Idcoach}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ coach.Idcoach) }}">
    <button class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>
</form>
